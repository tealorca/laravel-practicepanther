{
    "name": "tealorca/laravel-practicepanther",
    "description": "Simple Laravel Package to connect with PracticePanther",
    "keywords": [
        "tealorca",
        "practicepanther",
        "laravel-practicepanther"
    ],
    "homepage": "https://github.com/tealorca/laravel-practicepanther",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Daniel Felix",
            "email": "danielfelix1995@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tealorca\\LaravelPracticepanther\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tealorca\\LaravelPracticepanther\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tealorca\\LaravelPracticepanther\\LaravelPracticepantherServiceProvider"
            ],
            "aliases": {
                "LaravelPracticepanther": "Tealorca\\LaravelPracticepanther\\LaravelPracticepantherFacade"
            }
        }
    }
}
